{% extends "@AcMarcheDuobac/base.html.twig" %}

{% block body %}

    <div class="card">
        <h3 class="card-header">Pesées </h3>
        <div class="card-body">
            <p>
                <a class="nav-link" href="{{ path('duobac_pesee_all') }}">
                    Toutes les années
                </a>
            </p>

            <h5>Sélection par année</h5>
            <ul class="list-group">
                {% for year in years %}
                    <li class="list-group-item">
                        <a class="" href="{{ path('duobac_by_year', {'year':year}) }}">{{ year }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>

{% endblock %}
